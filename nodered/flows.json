        "id": "da87459b8d7e4cf7",
        "type": "function",
        "z": "cefe46de0f9e8bf2",
        "name": "Get TimeDiff in Millis",
        "func": "const deviceName = msg.payload.deviceInfo.deviceName;\nconst temperature = msg.payload.object.TEMP;\nconst timestamp = new Date(msg.payload.time).getTime()\n\nconst lastMCS = context.get(\"lastMCS\")||null;\nconst lastUSB = timestamp;\n\nif(temperature){\n    if(deviceName === \"LoRa-MCS\"){\n        context.set(\"lastMCS\", timestamp);\n        return;\n    }\n\n    if(deviceName === \"LoRa-USB\" && lastMCS != null){\n        msg.payload = {diffInMillis: lastUSB - lastMCS, time: timestamp};\n        msg.time = timestamp;\n        msg.measurement = \"Diff\"\n        context.set(\"lastMCS\", null);\n        return msg;\n    }\n}",
